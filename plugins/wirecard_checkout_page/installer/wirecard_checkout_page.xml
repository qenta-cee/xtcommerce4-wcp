    <db_install><![CDATA[
        include_once _SRV_WEBROOT . _SRV_WEB_PLUGINS. '/wirecard_checkout_page/installer/install.php';
]]></db_install>

    <plugin_code>
        <code>
            <hook>page_registry.php:bottom</hook>
            <phpcode><![CDATA[
define('PAGE_WIRECARD_CHECKOUT_PAGE_CHECKOUT', _SRV_WEB_PLUGINS.'wirecard_checkout_page/pages/wirecard_checkout_page_checkout.php');
    ]]></phpcode>
            <order>1</order>
            <active>1</active>
        </code>
        <code>
            <hook>class.checkout.php:_getPayment</hook>
            <phpcode><![CDATA[
      if(!empty($_COOKIE['QTAFFIWM'])){
        foreach($payment_data as $k => $v) {
          if ($k != 'wirecard_checkout_page') {
            unset($payment_data[$k]);
          }
        }
        reset($payment_data);
      }
    ]]></phpcode>
            <order>1</order>
            <active>1</active>
        </code>

        <code>
            <hook>class.checkout.php:_selectPayment_tpl_data</hook>
            <phpcode><![CDATA[
      require _SRV_WEBROOT._SRV_WEB_PLUGINS.'wirecard_checkout_page/hooks/checkout_selectpayment_tpl_data.php';
    ]]></phpcode>
            <order>1</order>
            <active>1</active>
        </code>

        <code>
            <hook>order_edit.php:display_data</hook>
            <phpcode><![CDATA[
            require _SRV_WEBROOT._SRV_WEB_PLUGINS.'wirecard_checkout_page/hooks/order_edit_display_data.php';
    ]]></phpcode>
            <order>0</order>
            <active>1</active>
        </code>

